/^#.*/ d
/^[ \t]*$/ d
s/#.*$//
s/disable_\([^= \t]*\)/enable_\1=no/
/^enable_/ s/^/DEFAULT/p
/^with_/ s/^/DEFAULT/p

EOF
eval `sed -n -f conftest.sed <settings/$withval`
rm conftest.sed
else
{ { echo "$as_me:$LINENO: error: Setting \"$withval\" not available" >&5
echo "$as_me: error: Setting \"$withval\" not available" >&2;}
   { (exit 1); exit 1; }; }
fi

