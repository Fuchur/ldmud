    sent.h object.h interpret.h config.h port.h exec.h instrs.h datatypes.h \
    hosts/unix.h hosts/be/be.h hosts/amiga/amiga.h hosts/amiga/patchfloat.h \
    machine.h ./hosts/amiga/ixfile.h

