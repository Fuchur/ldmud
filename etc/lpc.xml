<keyword String="keywords" attribute="6" context="#stay" />
<DetectChar attribute="15" context="5" char="&quot;" />
</context>
<context name="String" attribute="14" lineEndContext="#pop">
<LineContinue attribute="0" context="#stay" />
<Detect2Chars char="\" char1="\" attribute="14" context="#stay" />
<Detect2Chars char="\" char1="&quot;" attribute="14" context="#stay" />
<DetectChar char="&quot;" attribute="14" context="#pop" />
</context>
<context name="String" attribute="15" lineEndContext="#pop">
<LineContinue attribute="0" context="#stay" />
