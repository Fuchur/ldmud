</context>
<context name="Comment1" attribute="1" lineEndContext="#pop">
<LineContinue attribute="1" context="#stay" />
<RegExpr attribute="3" context="#stay" String="^[A-Z]+:" />
</context>
<context name="Comment2" attribute="2" lineEndContext="#stay">
<Detect2Chars attribute="2" context="#pop" char="*" char1="/"/>
