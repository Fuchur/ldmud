<RegExpr String="0o[0-7]+" attribute="11" context="#stay" />
<Int attribute="12" context="#stay" />
<RegExpr String="#'[^\t ][^\t ,)}\]/]*" attribute="13" context="#stay" />
<DetectChar attribute="14" context="4" char="&quot;" />
